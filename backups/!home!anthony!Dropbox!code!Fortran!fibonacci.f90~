program fibonacci
  implicit none

  integer :: ii
  integer, parameter :: L=10**8
  integer, dimension(L) :: v
  real :: start, finish

  call cpu_time(start)
  v(1:2)=1

  do ii=3,L
     v(ii)=v(ii-1)+v(ii-2)
  end do

  call cpu_time(finish)
  print '("Time = ",f6.3," seconds.")',finish-start
  
end program fibonacci
