


#include <iostream>
using namespace std;

int minimum(int array[],int n)
{
    int x=array[0];

    for(int i=0;i<n;i=i+1)
    {
        if(x >= array[i])
        {
            x=array[i];
        }
    }

    return x;
}



#ifndef WEBSUBMIT
int main()
{

    int array[5]={7,10,20,4,5};
    cout << minimum(array, 5) << endl;

}
#endif //WEBSUBMIT


