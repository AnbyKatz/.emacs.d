\section{Complex Integration}
\label{sec:org97f8b2b}

\begin{frame}[label={sec:orgf681462}]{Orientation}
\end{frame}
\begin{frame}[label={sec:orgd94eb55}]{Parametrization}
\end{frame}
\begin{frame}[label={sec:orgedcf1aa}]{Cauchy's Integral Formula}
\begin{theorem}[Cauchy's Integral Formula]
  Suppose $f:U\subseteq \mathbb{C}\to\mathbb{C}$ is a holomorphic and $U$ an open subset. Then for the open ball $B(a,R)\subseteq U$ we have that
  \begin{align*}
    f(a) = \frac{1}{2\pi i}\int_{\pd B(a,R)}  \frac{f(z)}{z-a}\dd z
  \end{align*}
\end{theorem}
\end{frame}

\begin{frame}[label={sec:orgb1c9aba}]{Examples}
Let $f(z) = z^2$ which is holomorphic on $B(3,R)$. Calculate
\begin{align*}
  \frac{1}{2\pi}\oint_{\pd B(3,R)}\frac{z^2}{z-3}\dd z
\end{align*}
Here $a = 3$ is our pole. Applying CIF gives us that
\begin{align*}
f(a) &= \frac{1}{2\pi}\oint_{\pd B(a,R)}\frac{f(z)}{z-a}\dd z=\frac{1}{2\pi}\oint_{\pd B(3,R)}\frac{z^2}{z-3}\dd z\\
     &=  z^2\Big|_{z=3}\\
     &= 9
\end{align*}
\end{frame}
\begin{frame}[label={sec:orgf356860}]{Examples}
Let $f(z)=z\cos(z)$ which is holomorphic on $B(0,4)$. Calculate
\begin{align*}
  \frac{1}{2\pi}\oint_{\pd B(0,4)}\frac{z\cos(z)}{(z-i)(z-1)}\dd z
\end{align*}
Here's a clever trick
\begin{align*}
  \frac{1}{2\pi}\oint_{\pd B(0,4)}\frac{z\cos(z)}{(z-i)(z-1)}\dd z &= \frac{z\cos(z)}{z-i}\Big|_{z=1}+\frac{z\cos(z)}{z-1}\Big|_{z=i} \\
                                                     &= \frac{cos(1)}{1-i}+\frac{i\cos(i)}{i-1}
\end{align*}
\end{frame}
\begin{frame}[label={sec:org9e7157c}]{Laurent Series}
\end{frame}
\begin{frame}[label={sec:org9c488cc}]{Residue Theorem}
\begin{theorem}
  Suppose $f:U\subseteq \mathbb{C}\to \mathbb{C}$ is holomorphic on $U\setminus\{a_1,...,a_n\}$. $U$ an open set then we have that
  \begin{align*}
    \int_U f(z)\dd z = 2\pi i \sum_{k=1}^n \text{Res}\qty(f,a_k)
  \end{align*}
\end{theorem}
\end{frame}

