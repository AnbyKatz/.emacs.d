#!/usr/bin/env python3.5
import matplotlib.pyplot as pypl
import numpy as np
import math as m
from scipy.integrate import quad
from matplotlib import rc
from pylab import*
from sympy import*

x,y,z=symbols('x y z')

J=Matrix([ [-sinh(x)*sin(y)*cos(z),-sinh(x)*cos(y)*sin(z)],[-sinh(x)*sin(y)*cos(z),sinh(x)*cos(y)*cos(z)],[cosh(x)*cos(y),0] ])

pprint(transpose(J)*J)
